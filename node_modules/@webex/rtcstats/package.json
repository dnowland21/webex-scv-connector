{
  "name": "@webex/rtcstats",
  "version": "1.5.2",
  "main": "dist/rtcstats.js",
  "types": "dist/rtcstats.d.ts",
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "Philipp Hancke",
  "license": "MIT",
  "files": [
    "dist/**/*"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "scripts": {
    "build": "tsc",
    "release": "semantic-release",
    "prepare": "husky",
    "test:lint": "eslint **/*.ts --max-warnings=0 --no-warn-ignored",
    "fix:lint": "eslint **/*.ts --no-warn-ignored --fix",
    "transpile:validate": "tsc --noEmit",
    "fix:prettier": "prettier **/*.ts --write"
  },
  "lint-staged": {
    "*.ts": [
      "prettier **/*.ts --write",
      "eslint **/*.ts --max-warnings=0 --fix",
      "tsc --noEmit"
    ]
  },
  "engines": {
    "node": ">=22.0.0",
    "npm": "please-use-yarn",
    "yarn": ">=1.22.0"
  },
  "dependencies": {
    "@types/node": "^22.5.5",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@semantic-release/changelog": "^6.0.2",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^9.0.1",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "@typescript-eslint/eslint-plugin": "^8.29.0",
    "@typescript-eslint/parser": "^8.29.0",
    "eslint": "^9.23.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.5",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.0",
    "prettier": "^3.5.3",
    "semantic-release": "^19.0.5",
    "typescript": "^5.8.2"
  },
  "packageManager": "yarn@1.22.22"
}
