{
  "name": "@webex/web-media-effects",
  "version": "2.19.0",
  "description": "Media effects for JS SDKs",
  "source": "src/index.ts",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "scripts": {
    "build": "run-s clean compile",
    "clean": "yarn transpile:clean",
    "compile": "rollup -c ./rollup.config.js",
    "docs": "rimraf ./docs/* && typedoc",
    "fix": "run-s fix:*",
    "fix:prettier": "prettier \"src/**/*.ts\" --write",
    "fix:lint": "eslint src --ext .ts --fix",
    "prepare": "husky install",
    "prepublishOnly": "node scripts/prep-for-npm.js",
    "test": "run-s build test:*",
    "test:coverage": "jest --coverage",
    "test:lint": "eslint src --ext .ts --max-warnings=0",
    "test:prettier": "prettier \"src/**/*.ts\" --list-different",
    "test:spelling": "cspell \"{README.md,.github/*.md,src/**/*.ts}\"",
    "test:unit": "jest",
    "transpile": "tsc",
    "transpile:clean": "rimraf ./dist",
    "transpile:validate": "tsc --noemit",
    "transpile:watch": "tsc --watch",
    "watch": "rollup -c ./rollup.config.js -w"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/webex/web-media-effects"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "files": [
    "dist/**/*"
  ],
  "keywords": [],
  "author": "",
  "license": "SEE LICENSE IN LICENSE.md",
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "eslint --max-warnings=0",
      "cspell"
    ]
  },
  "engines": {
    "node": ">=14.0.0",
    "yarn": ">=1.19.1"
  },
  "dependencies": {
    "@webex/ladon-ts": "^4.3.0",
    "events": "^3.3.0",
    "js-logger": "^1.6.1",
    "typed-emitter": "^1.4.0",
    "uuid": "^9.0.1",
    "worker-timers": "^8.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.0.2",
    "@commitlint/config-conventional": "^16.0.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/pluginutils": "^5.0.5",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/jest": "^27.4.0",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "cspell": "^5.15.1",
    "eslint": "^8.6.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^25.3.4",
    "eslint-plugin-jsdoc": "^39.9.1",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "jest": "^27.4.7",
    "jest-canvas-mock": "^2.4.0",
    "jest-matchmedia-mock": "^1.1.0",
    "lint-staged": "^12.1.7",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.63.0",
    "rollup-plugin-banner": "^0.2.1",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-dotenv": "^0.3.0",
    "rollup-plugin-dts": "^4.1.0",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "rollup-plugin-typescript2": "^0.31.1",
    "ts-jest": "^27.1.2",
    "typedoc": "^0.22.10",
    "typescript": "^4.5.4"
  }
}
