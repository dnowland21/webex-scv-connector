{
  "name": "@webex/ladon-ts",
  "version": "4.5.1",
  "description": "WebexIntelligence computer vision library",
  "type": "module",
  "main": "build/module/index.js",
  "typings": "build/module/index.d.ts",
  "module": "build/module/index.js",
  "keywords": [],
  "license": "SEE LICENSE in LICENSE",
  "scripts": {
    "update-version": "node -p \"const fs = require('fs'); function handle_result(err){console.log(err);}; fs.writeFile('src/lib/version.ts', 'export const LIB_VERSION = ' + JSON.stringify(require('./package.json').version).replaceAll('\\\"', '\\'') + ';\\n', handle_result);\"",
    "build": "run-s build:*",
    "build:module": "tsc -p tsconfig.module.json",
    "build:worker": "npm --prefix ./worker run build",
    "build:test": "npm --prefix ./test run build",
    "fix": "run-s fix:*",
    "fix:prettier": "prettier \"src/**/*.ts\" --write",
    "fix:lint": "eslint src --ext .ts --fix",
    "test": "run-s build test:*",
    "test:lint": "eslint src --ext .ts",
    "test:prettier": "prettier \"src/**/*.ts\" --list-different",
    "check-cli": "run-s test diff-integration-tests check-integration-tests",
    "check-integration-tests": "run-s check-integration-test:*",
    "diff-integration-tests": "mkdir -p diff && rm -rf diff/test && cp -r test diff/test && rm -rf diff/test/test-*/.git && cd diff && git init --quiet && git add -A && git commit --quiet --no-verify --allow-empty -m 'WIP' && echo '\\n\\nCommitted most recent integration test output in the \"diff\" directory. Review the changes with \"cd diff && git diff HEAD\" or your preferred git diff viewer.'",
    "watch:build": "tsc -p tsconfig.module.json -w",
    "doc": "run-s doc:html && open-cli build/docs/index.html",
    "doc:html": "typedoc src/ --exclude **/*.spec.ts --out build/docs",
    "doc:json": "typedoc src/ --exclude **/*.spec.ts --json build/docs/typedoc.json",
    "doc:publish": "gh-pages -m \"[ci skip] Updates\" -d build/docs",
    "version": "standard-version",
    "test-release": "run-s test build doc:html",
    "prepare-release": "run-s test version update-version build doc:html doc:publish",
    "serve:worker": "npm --prefix ./worker run serve"
  },
  "devDependencies": {
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "chai": "^4.3.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-functional": "^3.0.2",
    "eslint-plugin-import": "^2.22.0",
    "gh-pages": "^3.1.0",
    "mocha": "^9.1.3",
    "npm-run-all": "^4.1.5",
    "open-cli": "^6.0.1",
    "parcel": "^2.12.0",
    "prettier": "^2.1.1",
    "standard-version": "^9.0.0",
    "typedoc": "^0.23.24",
    "typescript": "^4.7.4"
  },
  "files": [
    "build/main",
    "build/module",
    "!**/*.spec.*",
    "!**/*.json",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ],
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "prettier": {
    "singleQuote": true
  },
  "dependencies": {
    "onnxruntime-web": "^1.15.1"
  }
}
